{
  "name": "form-piler",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docker:comp": "docker-compose -f docker-compose.yml build",
    "build": "turbo run build --filter=frontend",
    "dev": "turbo run dev --filter=frontend --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "tsconfig": "workspace:*",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.6.10"
}
